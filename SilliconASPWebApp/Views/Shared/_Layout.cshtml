<!DOCTYPE html>
<html lang="sv-se">
<head>
    <meta charset="UTF-8">
    <meta name="description" content="Asp.NET Development">
    <meta name="keywords" content="HTML, CSS, JavaScript, C#, ASP.NET">
    <meta name="author" content="Fredrik Bengtsson">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://kit.fontawesome.com/41c8b474e2.css" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    <title>@ViewData["Title"] - SiliconAspMvc</title>
</head>
<body>

    <div class"wrapper">
        <header>
            <partial name="~/Views/Shared/Partials/_HeaderPartial.cshtml" />
        </header>

        <main>
            @RenderBody()
        </main>

        <footer>
            <partial name="~/Views/Shared/Partials/_FooterPartial.cshtml" />
        </footer>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>